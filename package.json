{
    "name": "mathyn-codes-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:docker": "docker run -p 4000:4000 mathyn-codes-website:latest",
        "minikube:load": "minikube image load mathyn-codes-website:latest",
        "minikube:expose": "minikube service mathyn-codes-website --url",
        "helm:install": "@powershell -Command ./k8s/install.ps1 dryRun=false",
        "helm:uninstall": "@powershell -Command ./k8s/uninstall.ps1",
        "helm:upgrade": "@powershell -Command ./k8s/upgrade.ps1",
        "build": "ng build",
        "build:docker": "docker build -t mathyn-codes-website .",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve:ssr:mathyn-codes-website": "node dist/mathyn-codes-website/server/server.mjs"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.0.0",
        "@angular/common": "^19.0.0",
        "@angular/compiler": "^19.0.0",
        "@angular/core": "^19.0.0",
        "@angular/forms": "^19.0.0",
        "@angular/platform-browser": "^19.0.0",
        "@angular/platform-browser-dynamic": "^19.0.0",
        "@angular/platform-server": "^19.0.0",
        "@angular/router": "^19.0.0",
        "@angular/ssr": "^19.0.4",
        "express": "^4.18.2",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.0.4",
        "@angular/cli": "^19.0.4",
        "@angular/compiler-cli": "^19.0.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/node": "^18.18.0",
        "jasmine-core": "~5.4.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.6.2"
    }
}